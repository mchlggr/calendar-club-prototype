# OpenAI API key for agent conversations
OPENAI_API_KEY=your_openai_api_key

# Eventbrite API key for event discovery
# Get your key at: https://www.eventbrite.com/platform/api-keys
EVENTBRITE_API_KEY=your_eventbrite_api_key

# Firecrawl API key for web scraping (Luma events, etc.)
# Get your key at: https://firecrawl.dev
FIRECRAWL_API_KEY=your_firecrawl_api_key

# Exa API key for semantic event search
# Get your key at: https://exa.ai
EXA_API_KEY=your_exa_api_key

# Google Calendar OAuth credentials
# Create credentials at: https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=http://localhost:3000/auth/google/callback

# Meetup OAuth credentials
# Create OAuth app at: https://www.meetup.com/api/oauth/list/
MEETUP_CLIENT_ID=your_meetup_client_id
MEETUP_CLIENT_SECRET=your_meetup_client_secret
MEETUP_ACCESS_TOKEN=your_meetup_access_token

# Database (optional - defaults to local SQLite files)
# For Turso: libsql://your-database.turso.io
# Leave empty for local development (uses conversations.db and event_cache.db)
DATABASE_URL=
TURSO_AUTH_TOKEN=
