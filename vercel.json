{
  "version": 2,
  "framework": "nextjs",
  "installCommand": "uv sync && cd frontend && npm install",
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/.next",
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api/index.py" },
    { "source": "/health", "destination": "/api/index.py" }
  ]
}
